{% stylesheet %}
  .font-18{
    font-size: 18px;
  }
  .mb-24{
    margin-bottom: 24px;
  }
{% endstylesheet %}

<section class="about-us-water-lantern">
  <div class="container">
    <div class="row align-items-center p-0 g-0">
      <div class="col-lg-6 col-12 pr-lg-3 mb-4 mb-lg-0">
        <img
          src="{{'about-us-our-team.png'| asset_url }}"
          alt="Photo of our event team"
          class="img-fluid w-100 object-fit-cover"
          loading="lazy"
          itemprop="image"
          width=""
          height=""
        />
      </div>
      <div class="col-lg-6 col-12 pl-lg-3">
        <h2 class="font-56 text-white fw-medium mb-24" itemprop="name">Our Team</h2>
        <p class="font-18 text-color-bd" itemprop="description">
          Behind every glowing event is a team of passionate organizers,
          artists, volunteers, and dreamers. We work year-round to make each
          festival a meaningful and seamless experience — and we’re constantly
          inspired by the people we meet along the way.
        </p>
      </div>
    </div>
  </div>
</section>

